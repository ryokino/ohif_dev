{
  "version": 2,
  "name": "ohif-viewer",
  "builds": [
    {
      "src": "platform/viewer/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "platform/viewer/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/platform/viewer/dist/$1"
    }
  ]
}
